{
  "index_patterns": [
    "tracardi-event-*-*"
  ],
  "template": {
    "settings": {
      "number_of_shards": 5,
      "number_of_replicas": 1
    },
    "mappings": {
      "dynamic": false,
      "properties": {
        "id": {
          "type": "keyword"
        },
        "metadata": {
          "properties": {
            "time": {
              "properties": {
                "insert": {
                  "type": "date"
                }
              }
            },
            "ip": {
              "type": "keyword",
              "null_value": "NULL"
            },
            "status": {
              "type": "keyword",
              "null_value": "NULL"
            },
            "processed_by": {
              "properties": {
                "rules": {
                  "type": "keyword"
                },
                "third_party": {
                  "type": "keyword"
                }
              }
            },
            "profile_less": {
              "type": "boolean"
            }
          }
        },
        "type": {
          "type": "keyword",
          "null_value": "NULL"
        },
        "properties": {
          "dynamic": true,
          "type": "object"
        },
        "source": {
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "session": {
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "profile": {
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "aux": {
          "dynamic": true,
          "type": "object"
        },
        "tags": {
          "properties": {
            "values": {
              "type": "keyword"
            },
            "count": {
              "type": "double"
            }
          }
        }
      }
    }
  }
}